        <EditText android:id="@+id/EditText04" android:layout_width="match_parent" android:layout_height="wrap_content"/>
    </TableRow>
    <TableRow android:id="@+id/TableRow05" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:textSize="16px" android:id="@+id/TextView05" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/email"/>
        <EditText android:id="@+id/EditText05" android:layout_width="match_parent" android:layout_height="wrap_content"/>
    </TableRow>
    <TableRow android:id="@+id/TableRow06" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:textSize="16px" android:id="@+id/TextView06" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/blog"/>
        <EditText android:id="@+id/EditText06" android:layout_width="match_parent" android:layout_height="wrap_content"/>
    </TableRow>
    <TableRow android:id="@+id/TableRow07" android:layout_width="match_parent" android:layout_height="wrap_content">
        <Button android:textSize="16px" android:id="@+id/Button01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/ok"/>
        <Button android:textSize="16px" android:id="@+id/Button02" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/cancel"/>
    </TableRow>
</LinearLayout>
